# Example tfvars for ephemeralNotes Terraform
# Copy this to terraform.tfvars and edit values as needed.

env = "prod"
project = "ephemeralnotes"
location = "westeurope"
# Optional suffix to keep names unique across subscriptions/teams
naming_suffix = "001"

# SKUs
acr_sku = "Standard"
redis_sku = "Basic"
app_service_sku = "S1"

# Container image name (pushed to ACR)
image_name = "ephemeralnotes"
